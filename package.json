{
  "name": "remui",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.tsx",
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "15.8.1",
    "react": "^17.0.2",
    "react-cookie": "4.1.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "4.2.2",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
    "install:clean": "rm -rf node_modules | rm yarn.lock | rm package-lock.json | npm i",
    "start": "react-scripts-ts start",
    "local": "NODE_ENV=local REACT_APP_ENV=local NODE_OPTIONS='--inspect' react-scripts-ts start",
    "build": "react-scripts-ts build",
    "buildLocal": "NODE_ENV=local NODE_ENV=local REACT_APP_ENV=local react-scripts-ts build",
    "buildDev": "NODE_ENV=development NODE_ENV=development REACT_APP_ENV=dev react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "check-lint": "eslint . --ignore-path .gitignore --ignore-pattern /out --ignore-pattern /.next",
    "check-format": "prettier --list-different --ignore-path .gitignore .",
    "lint": "eslint . --fix --ignore-path .gitignore --ignore-pattern /out --ignore-pattern /.next",
    "format": "prettier --write --list-different --ignore-path .gitignore .",
    "validate": "npm-run-all --parallel lint format",
    "validate:buildLocal": "npm-run-all --parallel validate buildLocal",
    "validate:buildDev": "npm-run-all --parallel validate buildDev"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.61",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^16.14.23",
    "@types/react-dom": "^16.9.14",
    "@types/react-router-dom": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "keywords": [],
  "description": "responsive material ui kickstarter template"
}
